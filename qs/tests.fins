config:
  files:
    - ~/open-projects/github.com/hillwithsmallfields/qs/conf/accounts.yaml
    - ~/common/finances/conversions.yaml
  debug:
    trace:
      - ORACLE
      - TREVOR POTTS
      - D.DR SOUTH STAFFS WATER
      - D.DR E.ON
      - D.DR NHSBSA PPC
      - WRESTLERS
inputs:
  directory: ~/common/qs-scratch
  files:
    financisto-$SELECTION.csv:      app %s
    handelsbanken-$SELECTION.csv:   statement for %s
commands:
  - list_accounts("$OUTPUT/initial-account-names")
  - write_debug(app Handelsbanken current account, "$OUTPUT/handelsbanken-app.txt")
  - write_csv(app Handelsbanken current account, "$OUTPUT/handelsbanken-app.csv")
  - write_debug(statement for Handelsbanken current account, "$OUTPUT/handelsbanken-statement.txt")
  - write_csv(statement for Handelsbanken current account, "$OUTPUT/handelsbanken-statement.csv")
  - write_csv(by_day(app Handelsbanken current account), "$OUTPUT/handelsbanken-app-daily.csv")
  - write_csv(by_month(app Handelsbanken current account), "$OUTPUT/handelsbanken-app-monthly.csv")
  - write_csv(by_year(app Handelsbanken current account), "$OUTPUT/handelsbanken-app-yearly.csv")
  - write_csv(by_day(statement for Handelsbanken current account), "$OUTPUT/handelsbanken-statement-daily.csv")
  - write_csv(by_month(statement for Handelsbanken current account), "$OUTPUT/handelsbanken-statement-monthly.csv")
  - write_csv(by_year(statement for Handelsbanken current account), "$OUTPUT/handelsbanken-statement-yearly.csv")
  - write_csv(format_sheet(sheet(statement for Handelsbanken current account), "financisto"), "$OUTPUT/handelsbanken-statement-as-financisto.csv")
  - write_csv(add_sheet(app Handelsbanken current account, statement for Handelsbanken current account), "$OUTPUT/handelsbanken-added-from-statement.csv")
  - write_csv(format_sheet(sheet(add_sheet(app Handelsbanken current account, statement for Handelsbanken current account)), "financisto"), "$OUTPUT/handelsbanken-added-from-statement-as-financisto.csv")
  - write_csv(format_sheet(sheet(add_sheet(by_day(app Handelsbanken current account), by_day(statement for Handelsbanken current account))), "financisto"), "$OUTPUT/handelsbanken-added-from-statement-as-financisto-by-daily-summaries.csv")
  - write_csv(format_sheet(sheet(add_sheet(app Handelsbanken current account, statement for Handelsbanken current account, "regular")), "financisto"), "$OUTPUT/handelsbanken-regular-added-from-statement-as-financisto.csv")
  - write_csv(format_sheet(sheet(add_sheet(by_day(app Handelsbanken current account), by_day(statement for Handelsbanken current account), "regular")), "financisto"), "$OUTPUT/handelsbanken-regular-added-from-statement-as-financisto-by-daily-summaries.csv")
  - payees(statement for Handelsbanken current account, "$OUTPUT/statements-oracle.csv", "ORACLE")
  - payees(app Handelsbanken current account, "$OUTPUT/app-oracle.csv", "ORACLE")
  - list_accounts("$OUTPUT/final-account-names")
