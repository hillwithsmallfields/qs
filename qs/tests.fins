config:
  files:
    - ~/open-projects/github.com/hillwithsmallfields/qs/conf/accounts.yaml
    - ~/common/finances/conversions.yaml
  debug:
    trace:
      - ORACLE
      - TREVOR POTTS
      - D.DR SOUTH STAFFS WATER
      - D.DR E.ON
      - D.DR NHSBSA PPC
inputs:
  directory: ~/common/qs-scratch
  files:
    financisto.csv:      app %s
    handelsbanken.csv:   statement for %s
commands:
  - list_accounts("/tmp/account-names")
  - write_csv(app Handelsbanken current account, "/tmp/handelsbanken-app.csv")
  - write_csv(statement for Handelsbanken current account, "/tmp/handelsbanken-statement.csv")
  - write_csv(by_day(app Handelsbanken current account), "/tmp/handelsbanken-daily.csv")
  - write_csv(by_month(app Handelsbanken current account), "/tmp/handelsbanken-monthly.csv")
  - write_csv(by_year(app Handelsbanken current account), "/tmp/handelsbanken-yearly.csv")
  - write_csv(add_sheet(app Handelsbanken current account, statement for Handelsbanken current account), "/tmp/handelsbanken-added-from-statement.csv")
  - payees(statement for Handelsbanken current account, "/tmp/statements-oracle.csv", "ORACLE")
  - payees(app Handelsbanken current account, "/tmp/app-oracle.csv", "ORACLE")
