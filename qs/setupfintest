# -*- shell-script -*- mode please emacs

# Run this with source / . from the test script

echo Setting up fintests

ORIGINALS=$HOME/common/qs/qs-samples
SCRATCH=$HOME/common/qs/qs-scratch
CONFIGDIR=$HOME/open-projects/github.com/hillwithsmallfields/qs/conf
CONVDIR=$HOME/common/finances
QS=$HOME/open-projects/github.com/hillwithsmallfields/qs/qs
mkdir -p $SCRATCH
rm -rf $SCRATCH/*
for SOURCE in financisto handelsbanken
do
    for LEVEL in full sample-year sample-month sample-week
    do
        cp $ORIGINALS/$SOURCE/$SOURCE-$LEVEL.csv $SCRATCH
    done
done
IMPORTDATE=$(date --iso-8601)
echo Running version from $QS
OUTBASE=/tmp/fintest
rm -rf $OUTBASE/*
